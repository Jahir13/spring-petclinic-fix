<?xml version="1.0" encoding="UTF-8"?>
<!--
  PetClinic Load Test Plan - JMeter
  ==================================

  Este plan de pruebas incluye:
  1. Thread Group con ramp-up gradual
  2. Flujo 1: POST /owners/new con datos aleatorios
  3. Flujo 2: POST /owners/{id}/pets/new usando el ID extraído
  4. Extractores de datos (CSS/Regex)
  5. Listeners para métricas (Latencia, Throughput)

  Ejecutar desde línea de comandos:
  jmeter -n -t petclinic_loadtest.jmx -l results.jtl -e -o report/
-->
<jmeterTestPlan version="1.2" properties="5.0" jmeter="5.6.3">
  <hashTree>
    <TestPlan guiclass="TestPlanGui" testclass="TestPlan" testname="PetClinic Load Test Plan">
      <boolProp name="TestPlan.functional_mode">false</boolProp>
      <boolProp name="TestPlan.tearDown_on_shutdown">true</boolProp>
      <elementProp name="TestPlan.user_defined_variables" elementType="Arguments" guiclass="ArgumentsPanel" testclass="Arguments">
        <collectionProp name="Arguments.arguments">
          <elementProp name="BASE_URL" elementType="Argument">
            <stringProp name="Argument.name">BASE_URL</stringProp>
            <stringProp name="Argument.value">localhost</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
          <elementProp name="PORT" elementType="Argument">
            <stringProp name="Argument.name">PORT</stringProp>
            <stringProp name="Argument.value">8080</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
          <elementProp name="ERROR_THRESHOLD" elementType="Argument">
            <stringProp name="Argument.name">ERROR_THRESHOLD</stringProp>
            <stringProp name="Argument.value">2</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
        </collectionProp>
      </elementProp>
    </TestPlan>
    <hashTree>

      <!-- ============================================================ -->
      <!-- THREAD GROUP: Usuarios Concurrentes con Ramp-up Gradual      -->
      <!-- ============================================================ -->
      <ThreadGroup guiclass="ThreadGroupGui" testclass="ThreadGroup" testname="PetClinic Users - Gradual Ramp-up">
        <stringProp name="ThreadGroup.on_sample_error">continue</stringProp>
        <elementProp name="ThreadGroup.main_controller" elementType="LoopController" guiclass="LoopControlPanel" testclass="LoopController">
          <boolProp name="LoopController.continue_forever">false</boolProp>
          <!-- Número de iteraciones por usuario (0 = infinito) -->
          <intProp name="LoopController.loops">-1</intProp>
        </elementProp>
        <!-- Número total de usuarios (threads) -->
        <stringProp name="ThreadGroup.num_threads">100</stringProp>
        <!-- Ramp-up period: tiempo en segundos para alcanzar todos los usuarios -->
        <!-- Con 100 usuarios y 60 segundos, se agregan ~1.67 usuarios/segundo -->
        <stringProp name="ThreadGroup.ramp_time">60</stringProp>
        <!-- Scheduler para controlar duración total del test -->
        <boolProp name="ThreadGroup.scheduler">true</boolProp>
        <!-- Duración total del test en segundos (5 minutos) -->
        <stringProp name="ThreadGroup.duration">300</stringProp>
        <stringProp name="ThreadGroup.delay">0</stringProp>
        <boolProp name="ThreadGroup.same_user_on_next_iteration">true</boolProp>
      </ThreadGroup>
      <hashTree>

        <!-- HTTP Request Defaults -->
        <ConfigTestElement guiclass="HttpDefaultsGui" testclass="ConfigTestElement" testname="HTTP Request Defaults">
          <elementProp name="HTTPsampler.Arguments" elementType="Arguments" guiclass="HTTPArgumentsPanel" testclass="Arguments">
            <collectionProp name="Arguments.arguments"/>
          </elementProp>
          <stringProp name="HTTPSampler.domain">${BASE_URL}</stringProp>
          <stringProp name="HTTPSampler.port">${PORT}</stringProp>
          <stringProp name="HTTPSampler.protocol">http</stringProp>
          <stringProp name="HTTPSampler.contentEncoding">UTF-8</stringProp>
          <stringProp name="HTTPSampler.implementation">HttpClient4</stringProp>
          <boolProp name="HTTPSampler.follow_redirects">false</boolProp>
        </ConfigTestElement>
        <hashTree/>

        <!-- HTTP Cookie Manager (mantener sesión) -->
        <CookieManager guiclass="CookiePanel" testclass="CookieManager" testname="HTTP Cookie Manager">
          <collectionProp name="CookieManager.cookies"/>
          <boolProp name="CookieManager.clearEachIteration">false</boolProp>
          <boolProp name="CookieManager.controlledByThreadGroup">false</boolProp>
        </CookieManager>
        <hashTree/>

        <!-- ============================================================ -->
        <!-- GENERADOR DE DATOS ALEATORIOS (Random Variables)            -->
        <!-- ============================================================ -->
        <RandomVariableConfig guiclass="TestBeanGUI" testclass="RandomVariableConfig" testname="Random Phone Number">
          <stringProp name="variableName">PHONE</stringProp>
          <stringProp name="minimumValue">1000000000</stringProp>
          <stringProp name="maximumValue">9999999999</stringProp>
          <stringProp name="outputFormat">0000000000</stringProp>
          <boolProp name="perThread">true</boolProp>
        </RandomVariableConfig>
        <hashTree/>

        <!-- CSV Data Set para nombres aleatorios (opcional) -->
        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate Random Data">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">
// Generar datos aleatorios para Owner
import org.apache.commons.lang3.RandomStringUtils

def firstNames = ['Juan', 'María', 'Carlos', 'Ana', 'Pedro', 'Luis', 'Sofia', 'Diego', 'Camila', 'Andrés']
def lastNames = ['García', 'Rodríguez', 'Martínez', 'López', 'González', 'Hernández', 'Pérez', 'Sánchez']
def cities = ['Quito', 'Guayaquil', 'Cuenca', 'Manta', 'Ambato', 'Loja', 'Riobamba', 'Machala']
def streets = ['Principal', 'Amazonas', 'República', 'Colón', 'Orellana', 'Patria', 'Naciones Unidas']
def petNames = ['Max', 'Bella', 'Luna', 'Charlie', 'Lucy', 'Cooper', 'Daisy', 'Rocky', 'Buddy', 'Sadie']

def random = new Random()

// Datos del Owner
vars.put('FIRST_NAME', firstNames[random.nextInt(firstNames.size())])
vars.put('LAST_NAME', lastNames[random.nextInt(lastNames.size())] + '_' + System.currentTimeMillis() % 10000)
vars.put('ADDRESS', (random.nextInt(900) + 100) + ' Calle ' + streets[random.nextInt(streets.size())])
vars.put('CITY', cities[random.nextInt(cities.size())])
vars.put('TELEPHONE', String.format('%010d', random.nextLong() % 10000000000L).take(10))

// Datos del Pet
vars.put('PET_NAME', petNames[random.nextInt(petNames.size())] + '_' + (random.nextInt(9000) + 1000))
vars.put('PET_TYPE', String.valueOf(random.nextInt(6) + 1)) // 1-6: cat, dog, lizard, snake, bird, hamster

// Fecha de nacimiento (últimos 10 años)
def year = 2015 + random.nextInt(10)
def month = String.format('%02d', random.nextInt(12) + 1)
def day = String.format('%02d', random.nextInt(28) + 1)
vars.put('PET_BIRTH_DATE', "${year}-${month}-${day}")
          </stringProp>
        </JSR223PreProcessor>
