            <!-- Debug: Verificar que se extrajo el ID -->
            <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Verify Owner ID Extracted">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="script">
def ownerId = vars.get('OWNER_ID')
if (ownerId == null || ownerId == 'NOT_FOUND' || !ownerId.isNumber()) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage("Failed to extract Owner ID. Got: " + ownerId)
} else {
    log.info("Successfully extracted Owner ID: " + ownerId)
}
              </stringProp>
